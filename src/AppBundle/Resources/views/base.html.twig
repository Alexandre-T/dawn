<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <title>{% block title %}{{ 'app.title' | trans }} &bull; {{ 'app.msg.welcome' | trans }}{% endblock %}</title>
        {% block stylesheets %}
            <!-- Latest compiled and minified CSS -->
            <link id="bootstrap-css" rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha.6/css/bootstrap.min.css" integrity="sha384-rwoIResjU2yc3z8GV/NPeZWAv56rSmLldC3R/AZzGRnGxQQKnKkoFVhFQhNUwEyJ" crossorigin="anonymous">
            <!-- Font Awesome -->
            <link id="fontawesome-css" rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" crossorigin="anonymous">
            <!-- Animate.CSS -->
            <link id="animate-css" rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.5.2/animate.min.css" crossorigin="anonymous">
            <!-- Lightbox v2 -->
            <link id="lightbox-css" rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/lightbox2/2.9.0/css/lightbox.min.css" crossorigin="anonymous">
            <!-- Dawn css -->
            <link id="dawn-css" rel="stylesheet" href="{{ asset('css/dawn.css') }}">
        {% endblock %}
        {% block favicons %}
            <!-- Favicon -->
            <link rel="apple-touch-icon" href="{{ asset('apple-touch-icon.png') }}" sizes="180x180">
            <link rel="mask-icon" href="{{ asset('safari-pinned-tab.svg') }}" color="#5bbad5">
            <meta name="apple-mobile-web-app-title" content="{{ 'app.title' | trans }}">
            <link rel="icon" type="image/png" href="{{ asset('favicon-32x32.png') }}" sizes="32x32">
            <link rel="icon" type="image/png" href="{{ asset('favicon-16x16.png') }}" sizes="16x16">
            <link rel="manifest" href="{{ asset('manifest.json') }}">
            <meta name="application-name" content="{{ 'app.title' | trans }}">
            <meta name="theme-color" content="#ffffff">
        {% endblock %}
    </head>
    <body>
        {% block background %}<div id="background"></div>{% endblock %}
        {% block loader %}{% endblock %}
        {% block header %}{% endblock %}
        {% block container %}
        <div class="container">
            {% block body %}{% endblock %}
        </div>
        {% endblock %}
        {% block footer %}{% endblock %}
        {{ include('AppBundle::flashMessages.html.twig') }}
        {% block javascripts %}
            <!-- jQuery -->
            <script id="jquery-js" src="https://code.jquery.com/jquery-3.1.1.min.js" integrity="sha256-hVVnYaiADRTO2PzUGmuLJr8BLUSjGIZsDYGmIJLv2b8=" crossorigin="anonymous"></script>
            <!-- Latest compiled and minified JavaScript -->
            <script id="tether-js" src="https://cdnjs.cloudflare.com/ajax/libs/tether/1.4.0/js/tether.min.js" integrity="sha384-DztdAPBWPRXSA/3eYEEUWrWCy7G5KFbe8fFjk5JAIxUYHKkDx6Qin1DkWx51bBrb" crossorigin="anonymous"></script>
            <script id="bootstrap-js" src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha.6/js/bootstrap.min.js" integrity="sha384-vBWWzlZJ8ea9aCX4pEW3rVHjgjt7zpkNpZk+02D9phzyeVkE+jo0ieGizqPLForn" crossorigin="anonymous"></script>
            <!-- Clipboard -->
            <script id="clipboard-js" src="https://cdnjs.cloudflare.com/ajax/libs/clipboard.js/1.5.16/clipboard.min.js"></script>
            <!-- Lightbox -->
            <script id="lightbox-js" src="https://cdnjs.cloudflare.com/ajax/libs/lightbox2/2.9.0/js/lightbox.min.js"></script>
            <!-- MAP Responsive -->
            <script id="rwd-js" src="https://cdnjs.cloudflare.com/ajax/libs/jQuery-rwdImageMaps/1.6/jquery.rwdImageMaps.min.js"></script>
            <!-- Notification -->
            <script id="notification-js" src="{{ asset('js/notification.js') }}"></script>
            <!-- Flash to improve -->
            <script id="init">
                $('#flash-messages').flashNotification('init');
                lightbox.option({
                    'albumLabel': '{{ 'txt.albumLabel' | trans }}',
                    'wrapAround': true
                });
            </script>
        {% endblock %}
        {% block script %}{% endblock %}
        {% block ajax %}{% endblock %}
    </body>
</html>
